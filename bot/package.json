{
  "name": "ticket-bot-discord",
  "version": "1.0.0",
  "description": "Discord Ticket Bot using SapphireJS",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "sapphire": "sapphire",
    "generate": "sapphire generate",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "watch:start": "tsc-watch --onSuccess \"npm run start\"",
    "prisma:generate": "prisma generate --schema=../shared/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=../shared/prisma/schema.prisma",
    "format": "prettier --write \"src/\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@kaname-png/plugin-subcommands-advanced": "^2.5.0",
    "@prisma/client": "^6.3.1",
    "@sapphire/decorators": "^6.1.1",
    "@sapphire/discord.js-utilities": "^7.3.1",
    "@sapphire/framework": "^5.3.2",
    "@sapphire/plugin-logger": "^4.0.2",
    "@sapphire/plugin-subcommands": "^7.0.1",
    "@sapphire/utilities": "^3.18.2",
    "@skyra/env-utilities": "^1.3.0",
    "axios": "^1.7.2",
    "colorette": "^2.0.20",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "prisma": "^6.3.1"
  },
  "devDependencies": {
    "@sapphire/cli": "^1.9.3",
    "@sapphire/prettier-config": "^2.0.0",
    "@sapphire/ts-config": "^5.0.1",
    "@types/node": "^20.17.5",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsc-watch": "^6.2.0",
    "typescript": "~5.4.5"
  },
  "prettier": "@sapphire/prettier-config"
}
